{% capture sectionInterval %}
  {% case secSt.top_padding %}
    {% when 'extraLarge' %} xl:tw-pt-24 lg:tw-pt-20 md:tw-pt-16 tw-pt-12
    {% when 'large' %} xl:tw-pt-18 lg:tw-pt-14 md:tw-pt-12 tw-pt-10
    {% when 'default' %} lg:tw-pt-9 md:tw-pt-6 tw-pt-4
    {% when 'medium' %} md:tw-pt-6 tw-pt-3
    {% when 'small' %} tw-pt-2
    {% when 'none' %} tw-pt-0
  {% endcase %}
  {% case secSt.bottom_padding %}
    {% when 'extraLarge' %} xl:tw-pb-24 lg:tw-pb-20 md:tw-pb-16 tw-pb-12
    {% when 'large' %} xl:tw-pb-18 lg:tw-pb-14 md:tw-pb-12 tw-pb-10
    {% when 'default' %} lg:tw-pb-9 md:tw-pb-6 tw-pb-4
    {% when 'medium' %} md:tw-pb-6 tw-pb-3
    {% when 'small' %} tw-pb-2
    {% when 'none' %} tw-pb-0
  {% endcase %}
{% endcapture %}

{{ sectionInterval | strip | remove: "  " | remove: "	" }}
