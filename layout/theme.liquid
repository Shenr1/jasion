<!-- Event snippet for 开始结账 conversion page -->
<script>
document.querySelectorAll('button[type="submit"][name="checkout"]').forEach(function(e){
    e.addEventListener('click', function(){     
        console.log('begin checkout');
        gtag('event','conversion',{
            'send_to':'11458684466/AN_jCLiEtoMZELLM9tcq'
        })
    });
});
</script>


<!-- Event snippet for 加购 conversion page -->

<script>
document.querySelectorAll('form[action*="/cart/add"] button[type="submit"]').forEach(function(e){
    e.addEventListener('click', function(){     
        console.log('add to cart');
        gtag('event','conversion',{
            'send_to':'AW-11458684466/UtjLCLWEtoMZELLM9tcq'
        })
    });
});
</script>

