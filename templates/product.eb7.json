    "284f41b4-084a-4a8d-8976-b3fa9fef9ee6": {
      "type": "product-modes",
      "blocks": {
        "template--18321957847314__284f41b4-084a-4a8d-8976-b3fa9fef9ee6-16798008482be15754-0": {
          "type": "colum",
          "settings": {
            "title": "01. Pedal Assist Mode",
            "text": "Use the display to adjust the assist level from 1-5 to meet your daily needs."
          }
        },
        "template--18321957847314__284f41b4-084a-4a8d-8976-b3fa9fef9ee6-16798008482be15754-1": {
          "type": "colum",
          "settings": {
            "title": "02. Half Twist Throttle",
            "text": "Use the Half Twist Throttle to accelerate your bike freely."
          }
        },
        "template--18321957847314__284f41b4-084a-4a8d-8976-b3fa9fef9ee6-16798008482be15754-2": {
          "type": "colum",
          "settings": {
