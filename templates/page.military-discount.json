    "77b88c00-f313-4f1e-8cef-148e8733406a": {
      "type": "accordion-content",
      "blocks": {
        "template--20587020288293__77b88c00-f313-4f1e-8cef-148e8733406a-item-1": {
          "type": "item",
          "settings": {
            "title": "Military Verification",
            "content": "<p>Valid Documents:<\/p><p>1. DD-214 (you may block SSN)<\/p><p>2. LES, DFAS or TRICARE Document<\/p><p>3. Veterans Affairs ID or VA Postal Letter<\/p><p>4. USAA Insurance Card<\/p><p>5. Navy Federal Statement<\/p><p>6. Driver's License showing Veteran mark (requires visit to DMV to add it)<\/p><p>7. G.I. Bill Document<\/p>"
          }
        },
        "template--20587020288293__77b88c00-f313-4f1e-8cef-148e8733406a-item-2": {
          "type": "item",
          "settings": {
            "title": "Responder Verification",
            "content": "<p>Valid Documents:<\/p><p>1. Official ID Card<\/p><p>2. Official Badge (must have your name on it)<\/p><p>3. Pay stub from your employer<\/p><p>4. Promotion\/Award Certificate<\/p><p>5. Signed letter of employment status from your agency<\/p>"
          }
        },
        "template--20587020288293__77b88c00-f313-4f1e-8cef-148e8733406a-item-3": {
          "type": "item",
          "settings": {
            "title": "Medical Verification",
            "content": "<p>Valid Documents:<\/p><p>1. Official ID Card<\/p><p>2. Official Badge (must have your name on it)<\/p><p>3. Pay stub from your employer<\/p><p>4. Promotion\/Award Certificate<\/p><p>5. Signed letter of employment status from your agency<\/p>"
          }
        },
        "e621df88-8db3-4ab6-ab17-6c3641bcc6cd": {
          "type": "item",
          "settings": {
            "title": "Teacher Verification",
            "content": "<p>Valid Documents:<\/p><p>1. Official ID Card or Badge<\/p><p>2. Educator's License or State Certification<\/p><p>3. Credential used to gain access to your campus (must have your name on it)<\/p><p>4. Pay Stub from your employer<\/p><p>5. Signed letter of employment status from your school<\/p>"
          }
        },
        "aa47044d-ec00-4685-932f-c6735eaf688c": {
          "type": "item",
          "settings": {
            "title": "Student Verification",
            "content": "<p>1. A student ID card with a date<\/p>"
          }
        }
